{
  "name": "rolod0x",
  "version": "1.5.0",
  "description": "Your free private onchain address book",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rolod0x/rolod0x.git"
  },
  "bin": {
    "rolod0x": "./src/cli.ts"
  },
  "scripts": {
    "build": "tsc --noEmit && pnpm build:vite",
    "build:vite": "vite build",
    "build:firefox": "tsc --noEmit && pnpm build:firefox:vite",
    "build:firefox:vite": "cross-env __FIREFOX__=true vite build",
    "build:all": "tsc --noEmit && (run-p build:vite build:firefox:vite)",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "zip": "version=$(jq -r .version package.json); zip=rolod0x-v$version-chrome.zip; (cd dist/chrome && rm -f ../$zip && zip -r ../$zip *) && ls -l dist/$zip",
    "build:crx": "crx pack dist",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm build:hmr && (run-p wss build:watch)",
    "devtools": "pnpm react-devtools",
    "dev:firefox": "pnpm build:hmr && (run-p wss build:firefox:watch)",
    "dev:all": "pnpm build:hmr && (run-p wss build:watch build:firefox:watch)",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:firefox": "pnpm web-ext run -s dist/firefox",
    "commitlint": "commitlint --edit",
    "lint": "run-p -c lint:eslint lint:prettier",
    "lint:watch": "while true; do fd -e ts -e tsx -e js | entr -d pnpm lint; done",
    "lint:fix": "run-p lint:eslint:fix lint:prettier:fix",
    "lint:eslint": "eslint src --max-warnings 0 --ext .ts,.js,.tsx,.jsx",
    "lint:eslint:fix": "pnpm eslint --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "tokenlist:import": "pnpm exec tsx ./src/import-tokenlist.ts",
    "prepare": "husky install",
    "rolod0x": "pnpm exec tsx ./src/cli.ts",
    "website:prepare": "./scripts/website-prepare.sh"
  },
  "type": "module",
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/state": "^6.4.1",
    "@commander-js/extra-typings": "^12.0.1",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource-variable/open-sans": "^5.0.29",
    "@fontsource/roboto": "^5.0.13",
    "@fontsource/ubuntu": "^5.0.13",
    "@fontsource/ubuntu-mono": "^5.0.20",
    "@lezer/highlight": "^1.2.0",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/system": "^5.15.15",
    "@types/lodash": "^4.17.1",
    "@types/webextension-polyfill": "^0.10.7",
    "@uiw/codemirror-themes": "^4.22.2",
    "@uiw/react-codemirror": "^4.22.0",
    "construct-style-sheets-polyfill": "^3.1.0",
    "dedent": "^1.5.3",
    "ethers": "^6.12.1",
    "lodash": "^4.17.21",
    "murmurhash": "^2.0.1",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-router-dom": "^6.23.1",
    "tsx": "^4.11.0",
    "webext-dynamic-content-scripts": "^10.0.1",
    "webext-options-sync": "^4.2.1",
    "webext-permission-toggle": "5.0.0",
    "webext-permissions": "^3.1.3",
    "webext-tools": "^1.2.3",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@codemirror/view": "^6.26.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/dom": "^10.2.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@types/chrome": "^0.0.268",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-console": "^2.0.0",
    "lint-staged": "^15.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "react-devtools": "^5.2.0",
    "rollup": "^4.17.2",
    "sass": "^1.77.5",
    "ts-jest": "^29.1.4",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-checker": "^0.6.4",
    "web-ext": "^8.2.0",
    "ws": "^8.17.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ]
  },
  "packageManager": "pnpm@8.9.2",
  "peerDependencies": {
    "@codemirror/view": "^6.26.3"
  }
}
